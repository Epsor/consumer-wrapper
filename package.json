{
  "name": "@epsor/consumer-wrapper",
  "version": "2.0.5",
  "description": "Event consumer",
  "homepage": "https://github.com/Epsor/epsor-v2/tree/master/packages/tunnel-b2c-api-test-kafka-stream#readme",
  "bugs": {
    "url": "https://github.com/Epsor/epsor-v2/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Epsor/epsor-v2.git"
  },
  "license": "ISC",
  "author": "Epsor",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel src -d dist --ignore 'src/**/*.test.js'",
    "clean": "rm -rf dist",
    "commit": "git-cz",
    "lint": "eslint src --cache --max-warnings=0 --cache-location .eslintcache",
    "prepare": "npm run build",
    "start": "babel src -d dist --watch --copy-files --verbose",
    "test": "NODE_ENV=test jest --coverage"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run lint",
      "pre-push": "npm run test"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/git-cz"
    }
  },
  "dependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/preset-env": "^7.5.4",
    "@epsor/dto": "^2.0.8",
    "@epsor/kafka-producer": "^2.0.3",
    "@epsor/kafka-streams": "^2.0.3",
    "@epsor/logger": "^1.0.0",
    "@epsor/mongodb-wrapper": "^2.0.3",
    "aigle": "^1.14.0",
    "mongodb": "^3.2.7",
    "nodemon": "^1.19.1",
    "redis": "^2.8.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^8.1.0",
    "@shelf/jest-mongodb": "^1.1.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "commitizen": "^4.0.3",
    "commitlint": "^8.1.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.12.4",
    "git-cz": "^3.2.1",
    "husky": "^3.0.2",
    "jest": "^24.8.0",
    "jest-expo": "^33.0.2",
    "prettier": "^1.18.2"
  }
}
